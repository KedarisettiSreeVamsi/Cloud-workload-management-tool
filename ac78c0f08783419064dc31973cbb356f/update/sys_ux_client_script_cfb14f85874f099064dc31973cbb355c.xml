    <sys_ux_client_script action="INSERT_OR_UPDATE">
        <includes/>
        <macroponent display_value="Modal - Create Change Request">c6b10f85874f099064dc31973cbb354d</macroponent>
        <name>Set deployment time</name>
        <required_translations/>
        <script><![CDATA[/**
 * @param {params} params
 * @param {api} params.api
 * @param {any} params.event
 * @param {any} params.imports
 */
function handler({api, event, helpers, imports}) {
    var date = event.payload.value;
    if (date == null || date == "")
        return;
    if (new Date(date).getTime() > new Date().getTime())
        api.setState('deploymentTime', date);
}]]></script>
